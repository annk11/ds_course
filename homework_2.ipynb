   "source": [
    "import math\n",
    "\n",
    "a = 1\n",
    "b = -5\n",
    "c = 6\n",
    "\n",
    "D = b^2 - 4 * a * c\n",
    "\n",
    "if D > 0:\n",
    "    x1 = (-b - math.sqrt(D))/(2 * a)\n",
    "    x2 = (-b + math.sqrt(D))/(2 * a)\n",
    "    print(f'x1 = {x1}')\n",
    "    print(f'x2 = {x2}')\n",
    "elif D == 0:\n",
    "    x = (-b)/(2 * a)\n",
    "    print(f'x = {x}')\n",
    "else:\n",
    "    print('Решений нет')"
   ]
